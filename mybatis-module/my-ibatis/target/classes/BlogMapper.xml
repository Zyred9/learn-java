<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ibatis.mapper.BlogMapper">

    <select id="selectById" resultType="com.example.ibatis.doman.Blog">
        SELECT bid, name, author_id FROM blog where id = #{id}
